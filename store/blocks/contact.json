{
  "store.custom#atencio": {
    "blocks": [
      
    ]
  },   
  "store.custom#contact": {
      "blocks": [
        "flex-layout.row#contact-page-title",
        "flex-layout.row#contact-page"
      ]
    },
    "flex-layout.row#contact-page-title": {
      "children": [
        "rich-text#contact-page-title"
      ],
      "title": "Contact Page"
    },
    
    "flex-layout.row#contact-page": {
      "children": [
        "flex-layout.col#contact-form"
      ],
      "props":{
        "horizontalAlign": "middle"
      },
      "title": "Contact Page"
    },
    "rich-text#contact-page-title": {
        "props": {
          "text":
          "CONTACTA AMB NOSALTRES Fes-nos arribar els teus dubtes sobre productes, botigues i serveis a través del següent formulari:",
          "blockClass": "h2-institutional-page-title"
        }
    },
    "flex-layout.col#contact-form": {
        "children": [
          "form#contact"
        ],
        "props":{
          "horizontalAlign": "middle",
          "width": "50%"
        }
    },
    "form#contact": {
      "props":{
        "entity": "registroclub",
        "schema": "cadastroweb"
      },
      "blocks": ["form-success"] 
    }
}